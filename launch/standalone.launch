<?xml version="1.0"?>
<!--XML-->
<launch>
    <node pkg="point_cloud_io" type="read" name="pcl_reader" output="screen">
         <param name="file_path" value="$(find halcon_match)halconmatcher/test/test_scene.ply" />
         <param name="topic" value="points2" />
         <param name="frame" value="sensor_frame" />
         <param name="rate" value="0.2" />
    </node>

    <node pkg="halcon_match" type="matcher" name="halcon_matcher" output="screen" >
            <param name="reference_model" value="$(find halcon_match)halconmatcher/test/reference_model.stl" />
            <remap from="/halcon_matcher/points2" to="/pcl_reader/points2" />
    </node>
</launch>